{
  "name": "engine",
  "version": "0.0.1",
  "description": "Using latest JavaScript features on the server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdriqi/engine"
  },
  "author": "Rahmat Albariqi <ra@evius.id>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@evius/tus-server": "^0.3.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-bandwidth": "0.0.1",
    "express-subdomain": "^1.0.5",
    "express-transform": "^1.0.1",
    "file-type": "^9.0.0",
    "generate-schema": "^2.6.0",
    "js-beautify": "^1.7.5",
    "jsonwebtoken": "^8.2.2",
    "lokijs": "^1.5.5",
    "mime": "^2.3.1",
    "mongoose": "^5.2.4",
    "mongoose-autopopulate": "^0.8.0",
    "mongoose-hidden": "^1.5.3",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.1",
    "nedb": "^1.8.0",
    "nodemailer": "^5.0.0",
    "object-hash": "^1.3.0",
    "pug": "^2.0.0-beta11",
    "rimraf": "^2.6.2",
    "sharp": "^0.21.1",
    "transfer-rate": "^2.2.0",
    "uid-generator": "^1.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^3.1.3",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2,
      "no-console": 0
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "directories": {
    "test": "test"
  }
}
